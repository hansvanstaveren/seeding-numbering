#
# Set parameters for group sizing
#
grpsizes="28x13 4 3"

totalentries=`wc -l <seedin`
totalgroups=`echo $grpsizes | sed -e 's/  */+/g' -e 's/x/*/g'|bc`

if [ $totalentries -eq $totalgroups ]
then
    echo Will seed $totalentries pairs
    ../seeding -w $grpsizes <seedin
else
    echo entries $totalentries groups $totalgroups
    echo mismatch!
fi
